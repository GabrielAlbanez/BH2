{
    "version": 2,
    "builds": [
      {
        "src": "./src/index.ts", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/src/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ],
    "env": {
      "DATABASE_URL": "file:dev.db",
      "PORT" : 8080
   
    },
    "functions": {
      "api/**/*.ts": {
        "runtime": "@vercel/node"
      }
    }
  }